<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<fx:root maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="50.0"
  minWidth="50.0" type="BorderPane" xmlns="http://javafx.com/javafx/8.0.162-ea"
  xmlns:fx="http://javafx.com/fxml/1">
  <center>
    <GridPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="50.0"
      minWidth="50.0" BorderPane.alignment="CENTER">
      <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"/>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"/>
      </columnConstraints>
      <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
      </rowConstraints>
      <children>
        <JFXButton maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
          onAction="#onPrevious" prefWidth="200.0" text="Previous Step"/>
        <JFXButton maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
          onAction="#onNext" prefWidth="200.0" text="Next Step" GridPane.columnIndex="1"/>
        <JFXButton maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
          onAction="#onExecute" prefWidth="200.0" text="Execute" GridPane.rowIndex="1"/>
      </children>
    </GridPane>
  </center>
</fx:root>
